    <!-- CONTENT -->
    <section id="content">

        <!-- MAIN -->
        <main>
            <div class="head-title">
                <div class="left">
                    <h1>Edit product</h1>
                    <!-- <ul class="breadcrumb">
						<li>
							<a href="#">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right' ></i></li>
						<li>
							<a class="active" href="#">Home</a>
						</li>
					</ul> -->
                </div>

            </div>


            <div class="container " style="background-color: rgb(233, 230, 221);">
                <div class="container-head p-2 text-end">
                   <a href="">Edit Product</a>
                </div>
                <form action="/admin/editproduct" method="POST" style="background-color: antiquewhite;">
                    <div class="container " style="background-color: rgb(110, 107, 99);">
                        <div class="input-box">
                            <div>
                                <label for="pName">Product Name</label>
                                <input type="text" value="<%=Product.pname  %>" name="pname"  required>
                            </div>
                            
                            <div>
                                <label for="bName">Brand Name</label>
                                <input type="text" value="<%=Product.bname  %>" name="bname" required>
                            </div>
    
                            <div>
                                <label for="category">Category</label>
                                <select name="category" id="category">
                                <option value="<%=Product.category %>">Mobile</option>
                                </select>
                            </div>
                        </div>
        
                        <div class="input-box">
                            <div>
                                <label for="subTitle">SubTitle</label>                         
                                <input type="text" value="<%=Product.subtitle %>" name="subtitle" required>
                            </div>
        
                            <div>
                                <label for="descrption-head">Description Heading</label>
                                <input type="text" value="<%=Product.descriptionheading %>" name="descriptionheading">
                            </div>
                        </div>
        
                        <div class="input-box">
                            <div>
                                <label for="description">Description</label>
                                <input name="description" value="<%=Product.description %>"  rows="5" cols="50" class="description" required>
                                <!-- <textarea name="description"  rows="5" cols="50" class="description" required></textarea> -->
                            </div>
                        </div>
        
                        <div class="input-box">
                            <div>
                                <label for="firstPrice">First Price</label>
                                <input type="number" value="<%=Product.firstprice %>"  name="firstprice" required>
                            </div>
        
                            <div>
                                <label for="lastPrice">Last Price</label>
                                <input type="number" value="<%=Product.lastprice %>"  name="lastprice" required>
                            </div>
                            <div>
                                <label for="discount">Discount</label>
                                <input type="number" value="<%=Product.discount %>" name="discount" required>
                            </div>
                        </div>
        
                        <div class="input-box">
                            <div>
                                <label for="colour">Colour</label>
                                <input type="text" value="<%=Product.color %>" name="color" required>
                            </div>
        
                            <div>
                                <label for="inStock">In Stock</label>
                                <input type="number" value="<%=Product.instock %>" name="instock" required>
                            </div>
                        </div>
        
                        <div class="input-box">
                            <div>
                                <label for="image">Image</label>
                                <!-- <input type="file" va="<%=Product.image %>"  name="image" > -->
                                <img src="<%=Product.image%>" alt="gagag" style="width: 150px;" >
                            </div>
        
                            <!-- <div>
                                <input type="radio" name="new" value="1">
                                <label for="new">Newly launched</label>
                            </div> -->
                        </div>
        
                        <div class="btn-container">
                            <button class="btn" type="submit">Submit</button>
                        </div>
        
                    </div>
                </form>
                
            </div>


    </section>
    <!-- CONTENT -->


    <script src="script.js"></script>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
